#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55f066890bc0 .scope module, "counter_tb" "counter_tb" 2 10;
 .timescale -9 -12;
v0x55f0668a43b0_0 .var "clk", 0 0;
v0x55f0668a4470_0 .net "count", 3 0, v0x55f0668a4090_0;  1 drivers
v0x55f0668a4540_0 .var "enable", 0 0;
v0x55f0668a4640_0 .var "rst", 0 0;
S_0x55f066890d50 .scope module, "uut" "counter" 2 16, 3 5 0, S_0x55f066890bc0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "enable";
    .port_info 3 /OUTPUT 4 "count";
P_0x55f066890f30 .param/l "WIDTH" 0 3 5, +C4<00000000000000000000000000000100>;
v0x55f066856cf0_0 .net "clk", 0 0, v0x55f0668a43b0_0;  1 drivers
v0x55f0668a4090_0 .var "count", 3 0;
v0x55f0668a4170_0 .net "enable", 0 0, v0x55f0668a4540_0;  1 drivers
v0x55f0668a4240_0 .net "rst", 0 0, v0x55f0668a4640_0;  1 drivers
E_0x55f06688fed0 .event posedge, v0x55f0668a4240_0, v0x55f066856cf0_0;
    .scope S_0x55f066890d50;
T_0 ;
    %wait E_0x55f06688fed0;
    %load/vec4 v0x55f0668a4240_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55f0668a4090_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55f0668a4170_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x55f0668a4090_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55f0668a4090_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55f066890bc0;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x55f0668a43b0_0;
    %inv;
    %store/vec4 v0x55f0668a43b0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55f066890bc0;
T_2 ;
    %vpi_call 2 24 "$dumpfile", "counter_tb.vcd" {0 0 0};
    %vpi_call 2 25 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55f066890bc0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f0668a43b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f0668a4540_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f0668a4640_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f0668a4640_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f0668a4540_0, 0, 1;
    %delay 200000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f0668a4540_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f0668a4640_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f0668a4540_0, 0, 1;
    %delay 40000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f0668a4640_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f0668a4540_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f0668a4640_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f0668a4540_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f0668a4640_0, 0, 1;
    %vpi_call 2 49 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
